// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output = "./generated/test"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://test:test_pass@db_new_soil_test:5432/test_db"
}

enum UserType{
  SUDO
  USER
}

model User {
  user_id          String      @id @unique @default(uuid())
  login            String
  password         String
  userType        UserType

  @@map("users")
}
