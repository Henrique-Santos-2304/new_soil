// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL_LOCAL")
}

enum UserType{
  SUDO
  USER
}

model User {
  user_id          String      @id @unique @default(uuid())
  login            String
  password         String
  userType        UserType

  @@map("users")
}
